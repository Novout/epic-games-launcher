{
  "name": "epic-games-launcher",
  "productName": "Epic Launcher",
  "version": "1.0.0",
  "main": "main/index.js",
  "scripts": {
    "start": "electron .",
    "build-renderer": "next build renderer && next export renderer",
    "build-electron": "tsc -p electron-src",
    "build": "yarn build-renderer && yarn build-electron",
    "dist": "yarn build && electron-builder --dir",
    "type-check": "tsc"
  },
  "build": {
    "asar": false,
    "extraResources": [
      {
        "from": "renderer/out",
        "to": "app/renderer"
      }
    ]
  },
  "devDependencies": {
    "@types/next": "8.0.6",
    "@types/node": "^11.13.9",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.0.11",
    "electron": "^6.0.2",
    "electron-builder": "^21.2.0",
    "next": "latest",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "add": "^2.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "electron-is-dev": "^1.0.1",
    "electron-next": "^3.1.5",
    "emotion": "^10.0.17",
    "font-awesome": "^4.7.0",
    "formik": "^1.5.8",
    "global": "^4.4.0",
    "next-images": "^1.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.3.2",
    "topcoat": "^0.7.5"
  }
}
